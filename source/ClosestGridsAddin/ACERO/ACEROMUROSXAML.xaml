<Window x:Class="ClosestGridsAddinVANILLA.ACERO.ACEROMUROSUI"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:ClosestGridsAddinVANILLA.ACERO"
        mc:Ignorable="d"
        Title="ACERO DE REFUERZO EN MUROS" Height="294" Width="489">
    <Grid Background="Gray" Margin="0,0,-17,-65" RenderTransformOrigin="0.5,0.528">
        <Grid.RowDefinitions>
            <RowDefinition Height="96*"/>
            <RowDefinition Height="44*"/>
            <RowDefinition Height="23*"/>
            <RowDefinition Height="180*"/>
        </Grid.RowDefinitions>
        <ComboBox x:Name="TIPODEBARRAS" HorizontalAlignment="Left" Margin="83,0,0,0" VerticalAlignment="Center" Width="120" Grid.Row="1" Height="22"/>
        <TextBlock HorizontalAlignment="Left" Margin="83,76,0,0" Text="Tipo de barra" TextWrapping="Wrap" VerticalAlignment="Top" Height="16" Width="71"/>
        <TextBlock HorizontalAlignment="Left" Margin="83,20,0,0" Text="Recubrimiento :" TextWrapping="Wrap" VerticalAlignment="Top" Grid.RowSpan="2" Grid.Row="2" Height="16" Width="101"></TextBlock>
        <TextBox x:Name="RECUBRIMIENTO" HorizontalAlignment="Left" Margin="83,18,0,0" TextWrapping="Wrap" Text="0.025" VerticalAlignment="Top" Width="120" Grid.Row="3" Height="18"/>
        <TextBlock HorizontalAlignment="Left" Margin="279,54,0,0" Text="Barras Rectas :" TextWrapping="Wrap" VerticalAlignment="Top" Height="15" Width="93"></TextBlock>
        <TextBox x:Name="BARRASVERTICALES" HorizontalAlignment="Left" Margin="279,66,0,0" TextWrapping="Wrap" Text="1" VerticalAlignment="Top" Width="120" Height="18" />
        <TextBlock HorizontalAlignment="Left" Margin="279,17,0,0" Text="Barras diagonales :" TextWrapping="Wrap" VerticalAlignment="Top" Height="16" Width="117" Grid.Row="1"/>
        <TextBox x:Name="BARRASDIAGONALES" HorizontalAlignment="Left" Margin="279,0,0,0" TextWrapping="Wrap" Text="0.60" VerticalAlignment="Center" Width="120" Grid.Row="2" Height="18"/>
        <TextBlock HorizontalAlignment="Left" Margin="279,10,0,0" Text="Barras en U :" TextWrapping="Wrap" VerticalAlignment="Top" Grid.Row="3" Height="16" Width="84"></TextBlock>
        <TextBox x:Name="BARRASENU" HorizontalAlignment="Left" Margin="279,31,0,0" TextWrapping="Wrap" Text="0.50" VerticalAlignment="Top" Width="120" Grid.Row="3" Height="18"/>
        <Button Content="Aceptar" Click="Button_Click" Background="Black" Foreground="White" HorizontalAlignment="Left" Margin="143,85,0,0" VerticalAlignment="Top" RenderTransformOrigin="1.11,0.072" Height="22" Width="65" Grid.Row="3"/>
        <Button Content="Cancelar" Background="Black" Foreground="White" HorizontalAlignment="Left" Margin="288,85,0,0" VerticalAlignment="Top" RenderTransformOrigin="1.11,0.072" Height="22" Width="65" IsCancel="True" Grid.Row="3"/>

    </Grid>
</Window>
